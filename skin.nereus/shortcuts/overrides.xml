	<override action="globaloverride">
		<condition>String.IsEqual(Container(9000).ListItem.Property(ParentalControl),yes) + !String.IsEqual(Skin.String(mypassword),Skin.String(passcode))</condition>
		<action>Skin.SetString(dialog_action,::ACTION::)</action>
		<action>ActivateWindow(1133)</action> 
		<action>AlarmClock(parental,Skin.Reset(mypassword),$VAR[password_time],silent)</action>	
	</override>
